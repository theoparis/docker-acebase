{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "script"
    },
    "env": {
        "commonjs": true,
        "es6": true
    },
    "extends": ["plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "curly": ["warn", "multi"],
        "quotes": [2, "double"]
    }
}
